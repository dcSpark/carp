{
  "name": "oura-postgres-api",
  "version": "1.0.0",
  "description": "API for a Oura Postgres instance",
  "main": "app/index.js",
  "scripts": {
    "eslint": "eslint app",
    "test": "jest --config=jest.config.ts",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "dev": "yarn build && concurrently \"nodemon -x tsoa spec-and-routes\" \"nodemon\"",
    "build": "tsoa spec-and-routes && tsc --outDir build --experimentalDecorators",
    "start": "NODE_ENV=production node build/app/server.js",
    "parse-db": "pg_dump --no-owner --schema-only -d $DATABASE_URL > bin/schema.sql && npx zapatos",
    "prettier": "prettier --write \"**/*.tsx\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcSpark/oura-postgres-sink.git"
  },
  "author": "https://github.com/dcSpark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcSpark/oura-postgres-sink/issues"
  },
  "homepage": "https://github.com/dcSpark/oura-postgres-sink#readme",
  "dependencies": {
    "@dcspark/cardano-multiplatform-lib-nodejs": "0.1.0",
    "@prisma/client": "3.12.0",
    "body-parser": "1.20.0",
    "express": "4.17.3",
    "http-status-codes": "^2.2.0",
    "pg": "8.7.3",
    "swagger-ui-express": "4.3.0",
    "tsoa": "3.14.1",
    "typescript-is": "git+https://github.com/dcSpark/typescript-is-bin.git",
    "zapatos": "^5.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/pg": "8.6.5",
    "@types/prettier": "2.6.0",
    "@types/swagger-ui-express": "4.1.3",
    "@types/ts-nameof": "4.2.1",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "concurrently": "7.1.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "ts-jest": "27.1.4",
    "ts-nameof": "5.0.0",
    "ts-node": "10.7.0",
    "ttypescript": "1.5.13",
    "typescript": "4.6.3"
  }
}
